<?xml version="1.0" encoding="UTF-8"?>
<testSuite
   xmlns:tacbpel="http://enterprise.netbeans.org/bpel/TacService/tacService"
   xmlns:tacwsdl="http://j2ee.netbeans.org/wsdl/tacService"
   xmlns:tac="http://xml.netbeans.org/schema/tacService"
   xmlns="http://www.bpelunit.org/schema/testSuite">

  <name>tacService</name>
  <baseURL>http://localhost:7777/ws</baseURL>

  <deployment>
    <put name="tacService" type="testmode">
      <wsdl>tacService.wsdl</wsdl>
    </put>
    <partner name="tacService" wsdl="tacService.wsdl"/>
  </deployment>

  <setUp>
    <dataSource type="velocity" src="tacService-data.vm">
      <property name="iteratedVars">lines</property>
    </dataSource>
  </setUp>

  <testCases>
    <testCase name="MainTemplate" basedOn="" abstract="false" vary="true">
      <clientTrack>
        <sendReceive
            service="tacwsdl:tacService"
            port="tacServicePort"
            operation="reverseLines">

          <send>
            <template src="reverseLines.vm"/>
          </send>

          <receive/>
        </sendReceive>
      </clientTrack>
      <partnerTrack name="tacService">
        <receiveSend
          service="tacwsdl:tacService"
          port="tacServicePort"
          operation="reverseLines">
          <receive/>

          <send delaySequence="0,1">
            <template src="reverseLines-response.vm"/>
          </send>
        </receiveSend>
      </partnerTrack>
    </testCase>

  </testCases>
</testSuite>
